

[BASIC]
variable-rgx=[a-z0-9_]{1,30}$  # to accept 2 (dfferent) letters variables


Good-names=xx,dd,zz,hh,ww,pp,kk


[TYPECHECK]

disable=E1102,missing-docstring,useless-object-inheritance,duplicate-code,too-many-arguments,too-many-instance-attributes,too-many-locals,too-few-public-methods


# List of members which are set dynamically and missed by pylint inference
# system, and so shouldn't trigger E1101 when accessed. Python regular
# expressions are accepted.
generated-members=numpy.*,torch.*,cv2.*


[FORMAT]
max-line-length=120
